{
  "manifest_version": 3,
  "name": "Cookie Monster",
  "description": "Cookie Manager",
  "version": "0.0.1",
  "version_name": "alpha",
  "action": {
    "default_icon": "images/cookie.jpeg"
  },
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"]
    }
  ]
}
